{
  "name": "duxapp",
  "description": "duxapp模块，所有模块的公共基础模块",
  "version": "1.0.52",
  "dependencies": [],
  "npm": {
    "scripts": {
      "build:weapp": "duxapp runtime enterFile && taro build --type weapp",
      "build:swan": "duxapp runtime enterFile && taro build --type swan",
      "build:alipay": "duxapp runtime enterFile && taro build --type alipay",
      "build:tt": "duxapp runtime enterFile && taro build --type tt",
      "build:h5": "duxapp runtime enterFile && taro build --type h5",
      "build:qq": "duxapp runtime enterFile && taro build --type qq",
      "build:jd": "duxapp runtime enterFile && taro build --type jd",
      "build:quickapp": "duxapp runtime enterFile && taro build --type quickapp",
      "build:rn": "duxapp runtime enterFile && duxapp rn clearBuildAssets && taro build --type rn",
      "dev:weapp": "npm run build:weapp -- --watch",
      "dev:swan": "npm run build:swan -- --watch",
      "dev:alipay": "npm run build:alipay -- --watch",
      "dev:tt": "npm run build:tt -- --watch",
      "dev:h5": "npm run build:h5 -- --watch",
      "dev:qq": "npm run build:qq -- --watch",
      "dev:jd": "npm run build:jd -- --watch",
      "dev:quickapp": "npm run build:quickapp -- --watch",
      "dev:rn": "duxapp runtime enterFile && taro build --type rn --watch",
      "native-components:h5": "duxapp runtime enterFile && taro build native-components --type h5",
      "postinstall": "patch-package",
      "test": "jest",
      "lint": "eslint ."
    },
    "dependencies": {
      "@tarojs/components": "3.6.7",
      "@tarojs/helper": "3.6.7",
      "@tarojs/plugin-framework-react": "3.6.7",
      "@tarojs/plugin-platform-alipay": "3.6.7",
      "@tarojs/plugin-platform-h5": "3.6.7",
      "@tarojs/plugin-platform-jd": "3.6.7",
      "@tarojs/plugin-platform-qq": "3.6.7",
      "@tarojs/plugin-platform-swan": "3.6.7",
      "@tarojs/plugin-platform-tt": "3.6.7",
      "@tarojs/plugin-platform-weapp": "3.6.7",
      "@tarojs/runtime": "3.6.7",
      "@tarojs/shared": "3.6.7",
      "@tarojs/taro": "3.6.7",
      "classnames": "^2.3.2",
      "crypto-js": "^4.1.1",
      "dayjs": "^1.11.7",
      "preact": "~10.19.7",
      "qs": "6.9.7",
      "react": "^18.2.0",
      "react-dom": "^18.2.0",
      "vconsole": "^3.15.0"
    },
    "devDependencies": {
      "@babel/core": "^7.21.4",
      "@babel/preset-env": "^7.21.4",
      "@babel/runtime": "^7.21.0",
      "@prefresh/babel-plugin": "^0.4.1",
      "@prefresh/core": "^1.3.4",
      "@prefresh/webpack": "^3.2.3",
      "@tarojs/cli": "3.6.7",
      "@tarojs/webpack5-runner": "3.6.7",
      "@types/react": "^18.0.24",
      "@types/webpack-env": "^1.18.1",
      "babel-preset-taro": "3.6.7",
      "duxapp-cli": "^1.1.43",
      "eslint": "^8.26.0",
      "eslint-config-taro": "3.6.7",
      "eslint-plugin-import": "^2.26.0",
      "eslint-plugin-react": "^7.31.10",
      "eslint-plugin-react-hooks": "^4.6.0",
      "patch-package": "^6.5.0",
      "react-refresh": "^0.14.0",
      "stylelint": "^14.14.0",
      "webpack": "5.85.1"
    },
    "resolutions": {
      "@babel/core": "^7.21.4",
      "@babel/preset-env": "^7.21.4",
      "@babel/runtime": "^7.21.0",
      "qs": "6.9.7"
    }
  }
}
